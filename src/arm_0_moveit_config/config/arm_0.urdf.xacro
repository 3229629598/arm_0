<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="arm_0">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import arm_0 urdf file -->
    <xacro:include filename="$(find arm_0)/urdf/arm_0.urdf" />

    <!-- Import control_xacro -->
    <xacro:include filename="arm_0.ros2_control.xacro" />


    <xacro:arm_0_ros2_control name="FakeSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
